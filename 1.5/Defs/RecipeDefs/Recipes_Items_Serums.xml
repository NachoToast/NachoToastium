<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RecipeDef Name="NachoToastium_MakeSerumBase" Abstract="True">
        <jobString>Making mech serum.</jobString>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_CookMeal</soundWorking>
        <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
        <workSkill>Intellectual</workSkill>
        <researchPrerequisites>
            <li>NachoToastium_SerumEngineering</li>
        </researchPrerequisites>
        <recipeUsers>
            <li>DrugLab</li>
        </recipeUsers>
        <skillRequirements>
            <Crafting>10</Crafting>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Neutroamine</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>30</count>
            </li>
        </ingredients>
        <unfinishedThingDef>NachoToastium_UnfinishedMechSerum</unfinishedThingDef>
    </RecipeDef>

    <RecipeDef ParentName="NachoToastium_MakeSerumBase">
        <defName>NachoToastium_MakeHealerMechSerum</defName>
        <label>make healer mech serum</label>
        <description>Make a healer mech serum.</description>
        <workAmount>10000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>MedicineUltratech</li>
                    </thingDefs>
                </filter>
                <count>8</count>
            </li>
        </ingredients>
        <products>
            <MechSerumHealer>1</MechSerumHealer>
        </products>
    </RecipeDef>

