<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef Name="NachoToastium_ChipsteelRecipeBase" Abstract="True">
        <jobString>Mixing chipsteel.</jobString>
        <description>Mix together steel, uranium, and plasteel to form chipsteel.</description>
        <effectWorking>CutStone</effectWorking>
        <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <targetCountAdjustment>20</targetCountAdjustment>
        <recipeUsers>
            <li>TableStonecutter</li>
        </recipeUsers>
        <skillRequirements>
            <Crafting>10</Crafting>
        </skillRequirements>
    </RecipeDef>

    <RecipeDef ParentName="NachoToastium_ChipsteelRecipeBase">
        <defName>NachoToastium_MakeChipsteel</defName>
        <label>make chipsteel</label>
        <workAmount>2000</workAmount>
        <researchPrerequisites>
            <li>NachoToastium_ChipsteelMixing</li>
        </researchPrerequisites>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>30</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <products>
            <NachoToastium_Chipsteel>30</NachoToastium_Chipsteel>
        </products>
    </RecipeDef>

    <RecipeDef ParentName="NachoToastium_ChipsteelRecipeBase">
        <defName>NachoToastium_MakeChipsteelOptimized</defName>
        <label>make chipsteel (optimized)</label>
        <workAmount>2000</workAmount>
        <researchPrerequisites>
            <li>NachoToastium_ChipsteelAggregation</li>
        </researchPrerequisites>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>30</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <products>
            <NachoToastium_Chipsteel>90</NachoToastium_Chipsteel>
        </products>
    </RecipeDef>

</Defs>