<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- patches [FSF] Complex Jobs to show advanced drill as 'FSFDrilling' work type instead of mining -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[FSF] Complex Jobs</li>
        </mods>
        <match Class="PatchOperationReplace">
            <xpath>Defs/WorkGiverDef[defName="NachoToastium_Drill"]/workType</xpath>
            <value>
                <workType>FSFDrilling</workType>
            </value>
        </match>
    </Operation>

</Patch>